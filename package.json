{
  "name": "v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "server:dev": "NODE_OPTIONS='--no-warnings --loader ts-node/esm' nodemon --watch 'server/**/*.ts' -e ts --exec ts-node server/index.ts",
    "server:start": "NODE_ENV=$NODE_ENV ts-node --transpile-only server/index.ts",
    
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",

    "bundle:analyze": "ANALIZE=true next build",

    "db:generate": "prisma generate",
    "db:migrate:prod": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",

    "setup": "npm install && npm run db:generate",
    "build": "npm run db:generate && npm run db:migrate:prod && npm run next:build",
    "lint": "next lint",
    
    "dev": "npm run setup && npm run server:dev",
    "prod": "npm run build && npm run server:start",

    "start": " if [ \"$NODE_ENV\" = \"production\" ]; then npm run prod ; else npm run dev; fi"
  },
